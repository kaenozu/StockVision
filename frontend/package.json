{
  "name": "temp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:analyze": "npm run build && open bundle-analysis.html",
    "lint": "npx eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:fast": "PLAYWRIGHT_FAST_MODE=1 playwright test --project=chromium",
    "test:e2e:smoke": "playwright test --project=chromium --grep=\"@smoke\"",
    "test:e2e:critical": "playwright test --project=chromium tests/e2e/comprehensive/navigation.spec.ts tests/e2e/comprehensive/user-flows.spec.ts",
    "test:e2e:performance": "playwright test --project=chromium tests/e2e/comprehensive/performance.spec.ts",
    "test:e2e:parallel": "PLAYWRIGHT_WORKERS=4 playwright test --project=chromium",
    "test:e2e:debug": "PLAYWRIGHT_SLOW_MODE=1 playwright test --project=chromium --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:enhanced-report": "open test-results/enhanced-report.html",
    "test:e2e:dashboard": "tsx tests/e2e/tools/test-dashboard.ts",
    "test:e2e:cross-browser": "playwright test tests/e2e/cross-browser/",
    "test:e2e:mobile": "playwright test --project=\"Mobile Chrome\" --project=\"Mobile Safari\"",
    "test:e2e:desktop": "playwright test --project=chromium --project=firefox --project=webkit",
    "test:e2e:compatibility": "playwright test tests/e2e/cross-browser/browser-compatibility.spec.ts",
    "test:e2e:install": "playwright install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build:docs": "npm run build-storybook && mkdir -p docs/dist && cp -r storybook-static/* docs/dist/"
  },
  "dependencies": {
    "@sentry/react": "^8.38.0",
    "@types/node": "^24.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "axios": "^1.11.0",
    "chart.js": "^4.5.0",
    "highlight.js": "^11.11.1",
    "i18next": "^25.5.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "marked": "^16.2.1",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.1",
    "react-i18next": "^15.7.3",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.8.2",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^2.0.0",
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.55.0",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/addon-onboarding": "8.6.14",
    "@storybook/blocks": "^8.0.0",
    "@storybook/react": "^8.0.0",
    "@storybook/react-vite": "^8.0.0",
    "@storybook/test": "^8.0.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "dotenv": "^16.4.5",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.3",
    "storybook": "^8.0.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "vite": "^6.0.0",
    "vitest": "^3.2.4"
  }
}
