{
  "base_url": "https://docs.stockvision.com",
  "platforms": [
    {
      "platform": "vercel",
      "project_name": "stockvision-docs",
      "build_command": "npm run build",
      "output_directory": "dist",
      "custom_domain": "docs.stockvision.com",
      "ssl_enabled": true,
      "cdn_provider": "cloudflare",
      "environment_variables": {
        "NODE_ENV": "production",
        "NEXT_PUBLIC_API_URL": "https://api.stockvision.com"
      },
      "cache_settings": {
        "static_files": 31536000,
        "html_files": 3600,
        "api_responses": 300
      }
    },
    {
      "platform": "netlify",
      "project_name": "stockvision-docs-backup",
      "build_command": "npm run build:static",
      "output_directory": "public",
      "custom_domain": "docs-backup.stockvision.com",
      "ssl_enabled": true,
      "environment_variables": {
        "NODE_ENV": "production",
        "GATSBY_API_URL": "https://api.stockvision.com"
      },
      "cache_settings": {
        "max_age": 86400,
        "stale_while_revalidate": 3600
      }
    },
    {
      "platform": "aws_s3",
      "project_name": "stockvision-docs-s3",
      "output_directory": "build",
      "custom_domain": "s3-docs.stockvision.com",
      "ssl_enabled": true,
      "cdn_provider": "aws_cloudfront",
      "cache_settings": {
        "default_ttl": 86400,
        "max_ttl": 31536000,
        "min_ttl": 0
      }
    },
    {
      "platform": "github_pages",
      "project_name": "stockvision/docs",
      "output_directory": "docs",
      "custom_domain": "github-docs.stockvision.com",
      "ssl_enabled": true
    }
  ],
