  ],
  "cdn_settings": {
    "cloudflare": {
      "email": "${CLOUDFLARE_EMAIL}",
      "api_token": "${CLOUDFLARE_API_TOKEN}",
      "zone_id": "${CLOUDFLARE_ZONE_ID}",
      "cache_rules": {
        "*.html": {
          "cache_level": "standard",
          "ttl": 3600
        },
        "*.css,*.js,*.png,*.jpg,*.svg": {
          "cache_level": "aggressive", 
          "ttl": 31536000
        },
        "/api/*": {
          "cache_level": "bypass"
        }
      }
    },
    "aws_cloudfront": {
      "access_key_id": "${AWS_ACCESS_KEY_ID}",
      "secret_access_key": "${AWS_SECRET_ACCESS_KEY}",
      "region": "us-east-1",
      "price_class": "PriceClass_100",
      "cache_behaviors": [
        {
          "path_pattern": "*.css",
          "target_origin_id": "docs-origin",
          "viewer_protocol_policy": "redirect-to-https",
          "min_ttl": 31536000,
          "default_ttl": 31536000,
          "max_ttl": 31536000
        },
        {
          "path_pattern": "*.js",
          "target_origin_id": "docs-origin",
          "viewer_protocol_policy": "redirect-to-https",
          "min_ttl": 31536000,
          "default_ttl": 31536000,
          "max_ttl": 31536000
        },
        {
          "path_pattern": "*.html",
          "target_origin_id": "docs-origin",
          "viewer_protocol_policy": "redirect-to-https",
          "min_ttl": 0,
          "default_ttl": 3600,
          "max_ttl": 86400
        }
      ]
    }
  },
  "monitoring": {
    "enabled": true,
    "check_interval": 300,
    "performance_budget": {
      "lighthouse_score": 90,
      "page_load_time": 2.0,
      "first_contentful_paint": 1.0,
      "largest_contentful_paint": 2.5,
      "cumulative_layout_shift": 0.1,
      "first_input_delay": 100
    },
    "uptime_checks": [
      "https://docs.stockvision.com",
      "https://docs.stockvision.com/api/reference",
      "https://docs.stockvision.com/getting-started"
    ],
    "alerts": {
      "email": "ops@stockvision.com",
      "slack_webhook": "${SLACK_WEBHOOK_URL}",
      "discord_webhook": "${DISCORD_WEBHOOK_URL}"
    }
  },
  "seo": {
    "sitemap_enabled": true,
    "robots_txt_enabled": true,
    "structured_data_enabled": true,
    "open_graph_enabled": true,
    "twitter_cards_enabled": true,
    "canonical_urls_enabled": true,
    "meta_optimization_enabled": true,
    "disallowed_paths": [
      "/admin/",
      "/api/",
      "/.well-known/",
      "/temp/",
      "/cache/"
    ]
  },
  "build_optimization": {
    "minify_html": true,
    "minify_css": true,
    "minify_js": true,
    "compress_images": true,
    "generate_webp": true,
    "tree_shaking": true,
    "code_splitting": true,
    "service_worker": true,
    "pwa_enabled": true
  },
  "security": {
    "https_redirect": true,
    "hsts_enabled": true,
    "content_security_policy": "default-src 'self'; script-src 'self' 'unsafe-inline' https://cdn.jsdelivr.net; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com; font-src 'self' https://fonts.gstatic.com; img-src 'self' data: https:",
    "x_frame_options": "DENY",
    "x_content_type_options": "nosniff",
    "referrer_policy": "strict-origin-when-cross-origin"
  },
  "analytics": {
    "google_analytics": "${GOOGLE_ANALYTICS_ID}",
    "google_search_console": true,
    "hotjar": "${HOTJAR_ID}",
    "custom_events_enabled": true
  }
}