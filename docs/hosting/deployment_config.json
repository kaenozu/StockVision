      "title": "Changelog - StockVision Documentation",
      "description": "Latest updates, features, and improvements to the StockVision platform.",
      "og_title": "StockVision Changelog",
      "og_description": "Platform updates and release notes",
      "type": "article",
      "priority": 0.4,
      "changefreq": "weekly"
